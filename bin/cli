#!/usr/bin/env node

console.log("not implemented yet");
process.exit();

var tryPaths = ["~/.maestro/config", "/usr/local/etc/maestro/config"],
config,
celeri = require("celeri");

for(var i = tryPaths.length; i--;) {
	try {
		config = require(tryPaths[i].replace("~", process.env.HOME));
		break;
	} catch(e) {

	}
}

require("../").create(config, function(err, maestro) {
	if(err) {
		console.error(err.stack);
		process.exit();
	}
	celeri.onJs(maestro);	
});

celeri.open();

